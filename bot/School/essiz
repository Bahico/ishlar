@dp.callback_query_handler(text="An")
async def get_an(message: types.Message):
    await student_province_city_school.get_province(message, "Andijon", conn, vote_cb)
    await bot.delete_message(message.from_user.id, message.message.message_id)


@dp.callback_query_handler(text="Na")
async def get_na(message: types.Message):
    await student_province_city_school.get_province(message, "Namangan", conn, vote_cb)
    await bot.delete_message(message.from_user.id, message.message.message_id)


