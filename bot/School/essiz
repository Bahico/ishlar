@dp.message_handler(commands=['del_school'])
async def get_del_district(message: types.Message):
    person = conn.execute("select * from DISTRICT_GOVERNOR WHERE ID = (?);", (message.from_user.id,)).fetchall()
    if person:
        person = list(person[0])
        school = conn.execute("select * from DIRECTOR WHERE PROVINCE = (?) AND CITY = (?);",
                              (person[0], person[1])).fetchall()
        if school:
            School = InlineKeyboardMarkup(resize_keyboard=True)
            for i in school:
                School.add(InlineKeyboardButton(text=i[2],callback_data=vote_cb.new(name="del", province="school", city=i[1],school=i[2], tur="None", clas="None",action="di")))
            await bot.send_message(message.from_user.id, "Qaysi maktabni o'chirib yubormoqchisiz", reply_markup=School)
        else:
            await bot.send_message(message.from_user.id, "Siz hali maktab qo'shganingiz yo'q")

@dp.message_handler(commands=["add_school"])
async def get_add_school(message: types.Message):
    person = conn.execute("select * from DISTRICT_GOVERNOR where ID = (?);", (message.from_user.id,)).fetchall()
    if person:
        await bot.send_message(message.from_user.id, "Iltimos maktabning faqat raqamini kiriting!!!")
        await bot.send_message(message.from_user.id,"Maktab qo'shish uchun\nShu ko'rinishida yozing.\n\nmaktab_raqami,code,Ismi,Familiyasi")
        conn.execute("UPDATE DISTRICT_GOVERNOR SET TEXT = (?) WHERE ID = (?);", ("add_school", message.from_user.id,))
    else:
        await bot.send_message(message.from_user.id, "Siz tuman hokimi emassiz")
    await message.delete()
#   -----DISTRICT GOVERNOR CITY --------


@dp.message_handler(commands=['add'])
async def get_add(message: types.Message):
    await family_start.get_add(message, conn)
    await bot.delete_message(message.from_user.id, message.message_id)


@dp.message_handler(commands=['children'])
async def get_children(message: types.Message):
    await children.get_children(message, conn, vote_cb)

