<!--# async def get_governor(message: types.Message, conn, vote_cb, type):-->
<!--#     person = conn.execute("select * from GOVERNOR where ID = (?);", (message.from_user.id,)).fetchall()-->
<!--#     district = conn.execute("select * from DISTRICT_GOVERNOR where PROVINCE = (?);", (person[0][0],)).fetchall()-->
