<!--#     District.row_width = 3-->
<!--#     son = 0-->
<!--#     District.add(InlineKeyboardButton(text="Hammasi",-->
<!--#                                       callback_data=vote_cb.new(name="district", province=person[0][0], city="all",-->
<!--#                                                                 school="None", tur="None", clas="None",-->
<!--#                                                                 action="attend")))-->
<!--#     for i in district:-->
<!--#         son += 1-->
<!--#         District.insert(InlineKeyboardButton(text=i[1][2:],-->
<!--#                                              callback_data=vote_cb.new(name="district", province="start", city=i[1],-->
<!--#                                                                        school="None", tur="None", clas=type,-->
<!--#                                                                        action="attend")))-->
<!--#     if son != 0:-->
<!--#         await bot.send_message(message.from_user.id,-->
