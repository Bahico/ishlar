from aiogram.dispatcher import Dispatcher
from aiogram import Bot, types
from aiogram.types import InlineKeyboardMarkup, InlineKeyboardButton
from aiogram.utils import executor

from attend import main
from attend import attend_all
from config import TOKEN

# some code

bot = Bot(token=TOKEN)
dp = Dispatcher(bot)


async def get_attend_callback(message: types.Message, conn, vote_cb, callback_data):
    if callback_data["name"] == "director":
        director = conn.execute("select * from DIRECTOR where ID = (?);", (message.from_user.id,)).fetchall()
        await main.get_school(message, director[0][1], director[0][2], conn, vote_cb, "director")


    elif callback_data["name"] == "governor":
        await main.get_governor(message, conn, vote_cb, "governor")


    elif callback_data["name"] == "district_name":
        district = conn.execute("select * from DISTRICT_GOVERNOR where ID = (?);", (message.from_user.id,)).fetchall()
        await main.get_district(message, district[0][1], conn, vote_cb, "district")


    elif callback_data["name"] == "district":
        if callback_data["province"] != "start":
            if callback_data["city"] == "all":
                await main.get_student(message, callback_data["province"], None, None, None, None, vote_cb)

    if callback_data["province"] == "start":
        if callback_data["name"] == "district":
            await main.get_district(message, callback_data["city"], conn, vote_cb, callback_data["clas"])


        elif callback_data["name"] == "school":
            if callback_data["school"] == "all":
                await main.get_student(message, None, callback_data["city"], None, None, None, vote_cb)
            elif callback_data["school"] == "back":
                await main.get_governor(message, conn, vote_cb, callback_data["clas"])
            else:
                await main.get_school(message, callback_data["city"], callback_data["school"], conn, vote_cb,
                                      callback_data["clas"])


        elif callback_data["name"] == "class_n":
            type = callback_data["clas"].split(",")
            if type[0] == "all":
                await main.get_student(message, callback_data["city"], callback_data["school"], None, None, conn,
                                       vote_cb)
            elif type[0] == "back":
                await main.get_district(message, callback_data["city"], conn, vote_cb, type[1])
            else:
                await main.get_class(message, callback_data["city"], callback_data["school"], type[0], conn, vote_cb,
                                     type[1])


        elif callback_data["name"] == "class_tur":
            type = callback_data["clas"].split(",")
            if callback_data["tur"] == "back":
                await main.get_school(message, callback_data["city"], callback_data["school"], conn, vote_cb, type[1])
            else:
                await main.get_tur(message, callback_data["city"], callback_data["school"], type[0],
                                   callback_data["tur"], conn, vote_cb, type[1])


        elif callback_data["name"] == "student":
            if callback_data["tur"] == "back":
                await main.get_school(message, callback_data["city"], callback_data["school"], conn, vote_cb,
                                      callback_data["clas"])
            else:
                await main.get_student(message, "None", callback_data["city"], callback_data["school"],
                                       callback_data["clas"], callback_data["tur"], vote_cb)


    elif callback_data["province"] == "second":
        if callback_data["name"] == "do_not":
            if callback_data["tur"] != "None":
                await main.get_do_not_come(message, callback_data["city"], callback_data["school"],
                                           callback_data["clas"], callback_data["tur"], conn, vote_cb)
            else:
                await bot.send_message(message.from_user.id, "Hali bu funksiya ishga tushmagan")
        elif callback_data["name"] == "leave":
            if callback_data["tur"] != "None":
                await main.get_hour(message, int(callback_data["tur"]), conn)
            else:
                await bot.send_message(message.from_user.id, "Hali bu funksiya ishga tushmagan")
        elif callback_data["name"] == "date":
            if callback_data["school"] == "None" or callback_data["tur"] == "None" or callback_data["clas"] == "None":
                type = callback_data["city"].split('.')
                if len(type) == 2:
                    await attend_all.get_year(message, "None", type[0], callback_data["school"], conn, vote_cb)
                else:
                    await attend_all.get_year(message, type[0], "None", "None", conn, vote_cb)
            else:
                await main.get_date(message, int(callback_data["tur"]), conn, vote_cb)


    elif callback_data["province"] == "date":
        if callback_data["name"] == "year":
            if callback_data["tur"] == "all":
                await main.get_date_all(message, None, None, callback_data["city"], callback_data["school"],
                                        callback_data["clas"], conn)
            else:
                await main.get_date_month(message, callback_data["tur"], callback_data["clas"], callback_data["city"],
                                          callback_data["school"], conn, vote_cb)

        elif callback_data["name"] == "month":
            all = callback_data["tur"].split("/")
            if all[0] == "all":
                await main.get_date_all(message, all[1], None, callback_data["city"], callback_data["school"],
                                        callback_data["clas"], conn)
            else:
                await main.get_date_day(message, all[1], all[0], callback_data["clas"], callback_data["city"],
                                        callback_data["school"], conn, vote_cb)

        elif callback_data["name"] == "day":
            all = callback_data["tur"].split("/")
            if all[1] == "all":
                await main.get_date_all(message, all[2], all[0], callback_data["city"], callback_data["school"],
                                        callback_data["clas"], conn)
            else:
                await main.get_date_day_print(message, all[2], all[0], all[1], callback_data["clas"],
                                              callback_data["city"], callback_data["school"], conn, vote_cb)
    elif callback_data["name"] == "date":
        if callback_data["province"] == "year":
            if callback_data["school"] == "all":
                await attend_all.get_date_all(message, callback_data["city"], callback_data["tur"],
                                              callback_data["clas"], "None", "None", "None", conn)
            else:
                await attend_all.get_month(message, callback_data["school"], callback_data["city"],
                                           callback_data["tur"], callback_data["clas"], conn, vote_cb)
        elif callback_data["province"] == "month":
            type = callback_data["school"].split(",")
            if type[1] == "all":
                await attend_all.get_date_all(message, callback_data["city"], callback_data["tur"],
                                              callback_data["clas"], type[0], "None", "None", conn)
            elif type[0] == "back":
                await attend_all.get_year(message, callback_data["city"], callback_data["tur"], callback_data["clas"],
                                          conn, vote_cb)
            else:
                await attend_all.get_day(message, type[0], type[1], callback_data["city"], callback_data["tur"],
                                         callback_data["clas"], conn, vote_cb)
        elif callback_data["province"] == "day":
            type = callback_data["school"].split('/')
            if type[1] == "all":
                await attend_all.get_date_all(message, callback_data["city"], callback_data["tur"],
                                              callback_data["clas"], type[2], type[0], "None", conn)
            elif type[0] == "back":
                await attend_all.get_month(message, type[0], callback_data["city"], callback_data["tur"],
                                           callback_data["clas"], conn, vote_cb)
            else:
                await attend_all.get_date_all(message, callback_data["city"], callback_data["tur"],
                                              callback_data["clas"], type[2], type[0], type[1], conn)

    await bot.delete_message(message.from_user.id, message.message.message_id)
